    flex-wrap: wrap;
    max-height: 58%;
}

.health-tab {
    flex-grow: 0;
    position: relative;
    width: 50px;
    height: 50px;
    justify-content: center;
    flex-shrink: 1;
    margin-bottom: 12px;
    margin-left: -6px;
    background-image: url("project://database/Assets/Sprites/Z_OldPlaceholderSprites/newheart.png?fileID=2800000&guid=43b02e3f73b9646ee9e89fec4d82da5b&type=3#newheart");
}

.pillar-tab-text {
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
