    justify-content: center;
    flex-shrink: 1;
    margin-bottom: 12px;
    background-image: url("project://database/Assets/Sprites/Z_OldPlaceholderSprites/block_vector.png?fileID=2800000&guid=4b398c458377645acb90057ab391c591&type=3#block_vector");
}

.base-attack-tab {
    flex-grow: 0;
    position: relative;
    width: 37px;
