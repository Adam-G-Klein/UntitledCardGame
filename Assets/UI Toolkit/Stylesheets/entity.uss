}

.health-tab-text {
    margin-left: 0;
    margin-right: 0;
    margin-top: -3px;
    margin-bottom: -3px;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    color: rgb(255, 255, 255);
    font-size: 12px;
    -unity-font-style: bold;
    -unity-text-align: middle-center;
}

.base-attack-tab-text {
    background-color: rgba(255, 255, 255, 0.6);
}

.block-tab {
    flex-grow: 0;
    position: relative;
    width: 37px;
